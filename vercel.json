{
  "version": 2,
  "builds": [
    {
      "src": "src/api/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "node_modules/p-limit/**",
          "node_modules/yocto-queue/**"
        ]
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/api/index.ts"
    }
  ],
  "git": {
    "deploymentEnabled": false
  }
}
